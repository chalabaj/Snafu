[Settings]
natoms  = 3               # number of atoms in system
nstates = 1               # number of electronic states
init_state = 1            # initial electronic state
timestep = 0.05           #
maxsteps = 1000           # total number of steps
method  = adiabatic       # adiabatic / diabatization scheme
abinitio  = gaussian        # where to take gradients (gaussian, molpro...)
restart = 0